        <!--android:value="GlideModule" />-->
        <meta-data android:name="com.config.GlideConfig" android:value="GlideModule" />
        <!--Glide End-->

        <activity android:name="com.lesehome.example.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.lesehome.example.Example1Activity" />
